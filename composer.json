{
    "name": "bcrowe/growl",
    "type": "library",
    "description": "Growl notifications with PHP.",
    "keywords": ["php", "growl", "notifications"],
    "homepage": "https://github.com/bcrowe/growl",
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Crowe",
            "email": "bryan@bryan-crowe.com",
            "homepage": "http://bryan-crowe.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "2.3.*"
    },
    "autoload": {
        "psr-4": {
            "BryanCrowe\\Growl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BryanCrowe\\Growl\\": "src",
            "BryanCrowe\\Growl\\Test\\": "tests"
        }
    }
}
